<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="cj.com.gameanimal">

    <application

        android:allowBackup="true"
        android:icon="@drawable/lanch"
        android:label="@string/app_name"
        android:roundIcon="@drawable/lanch"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.NoActionBar">
        <activity
            android:screenOrientation="portrait"
            android:name=".MainActivity"
            android:launchMode="singleTask">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".Task1"
            android:screenOrientation="portrait">

        </activity>
        <activity
            android:name=".Task2"
            android:screenOrientation="portrait">

        </activity>
        <activity android:name=".Task3"
            android:screenOrientation="portrait">

        </activity>
        <activity android:name=".Task4"
            android:screenOrientation="portrait">

        </activity>
        <activity android:name=".Task5"
            android:screenOrientation="portrait">

        </activity>
        <activity android:name=".GameSuccess"
            android:screenOrientation="portrait">

        </activity>
        <activity android:name=".GamOver"
            android:screenOrientation="portrait">

        </activity>
        <activity
            android:name=".Setting"
            android:launchMode="singleInstance"
            android:screenOrientation="portrait">

        </activity>
        <service
            android:name=".BgMusicService"
            android:exported="true"
            android:process=":remote">

        </service>
    </application>

</manifest>